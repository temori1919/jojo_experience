{
  "manifest_version": 2,
  "name": "Jojo experience",
  "version": "1.0.0",
  "description": "スタンド使いになれるChrome Extensionです",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": [
      	"jquery.min.js",
      	"soundEffect.js",
      	"caretposition.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "images/*"
  ],
  "permissions": [
    "storage"
  ]
}
