{
  "manifest_version": 3,
  "name": "Jojo experience",
  "version": "1.1.1",
  "description": "スタンド使いになれるChrome Extensionです",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": [
      	"jquery.min.js",
      	"soundEffect.mjs",
      	"caretposition.js",
      	"jquery.keyframes.min.js",
        "extensions/extensionMethods.js"
      ]
    }
  ],
  "web_accessible_resources": [{
    "resources": [
      "images/*",
      "extensions/*"
    ],
    "matches" : ["<all_urls>"]
}],
  "permissions": [
    "storage"
  ]
}
